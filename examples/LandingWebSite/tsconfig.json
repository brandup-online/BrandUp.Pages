{
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "node",
    "target": "ESNext",
    "sourceMap": false,
    "strict": true,
    "lib": [ "ES2020", "DOM" ],
    "baseUrl": "./_client",
    "noEmitOnError": true,
    "declaration": false,
    "declarationMap": false
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    ".idea",
    ".vscode"
  ],
  "compileOnSave": true
}