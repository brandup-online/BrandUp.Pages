@model AppPageModel
<!DOCTYPE html>
<html lang="en" prefix="og: http://ogp.me/ns#">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link type="text/css" rel="stylesheet" href="~/dist/app.css" asp-append-version="true" />
</head>
<body class="landing">
    <script type="text/javascript" src="~/dist/app.js" asp-append-version="true"></script>

    <div class="app">
        @await Html.BlockAsync("page-nav", typeof(Contents.PageNav.PageNavContent))
        <main role="main" class="content-width app-content" page-content>
            @RenderBody()
        </main>
    </div>

    @await Html.BlockAsync("mobile-menu", typeof(Contents.MobileMenu.MobileMenuContent))
</body>
</html>